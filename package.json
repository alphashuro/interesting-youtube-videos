{
  "name": "interesting-youtube-videos",
  "version": "1.0.0",
  "description": "A webpage for listing interesting YouTube videos.",
  "scripts": {
    "test": "npm run lint && npm run testonly",
    "testonly": "mocha --require mocha.config.js assets/js/**/tests/**/*.js --compilers js:babel-core/register  && npm run test:django",
    "lint": "xo assets/js/**/* server.js",
    "build:prod": "webpack --config webpack/prod.config.js --progress --colors",
    "build:dev": "webpack --config webpack/dev.config.js --progress --colors",
    "watch": "node webpack/server.js",
    "check": "python manage.py check",
    "check:prod": "npm run check -- --deploy",
    "update_data": "python manage.py makemigrations && python manage.py migrate",
    "serve:dev": "python manage.py runserver 0.0.0.0:8000",
    "serve:prod": "PYTHON_ENV='production' python manage.py runserver 0.0.0.0:80",
    "start": "npm run build:prod && npm run serve:prod",
    "startdev": "npm run watch && npm run serve:dev",
    "test:django": "python manage.py test videos"
  },
  "xo": {
    "ignores": [
      "bundles/**/*",
      "node_modules/**/*"
    ],
    "env": [
      "browser",
      "node"
    ],
    "extends": "xo-react",
    "rules": {
      "react/jsx-no-bind": [
        0,
        {
          "ignoreRefs": true,
          "allowArrowFunctions": true
        }
      ]
    }
  },
  "keywords": [
    "youtube",
    "videos"
  ],
  "author": "Alpha Shuro",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.6.5",
    "chai": "^3.5.0",
    "enzyme": "^2.1.0",
    "eslint-plugin-react": "^4.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "sinon": "^1.17.3",
    "webpack": "^1.12.14",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-server": "^1.14.1",
    "xo": "^0.13.0"
  }
}
