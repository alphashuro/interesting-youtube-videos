{
  "name": "interesting-youtube-videos",
  "version": "1.0.0",
  "description": "A webpage for listing interesting YouTube videos.",
  "scripts": {
    "test": "npm run lint && npm run testonly",
    "testonly": "mocha --require mocha.config.js assets/js/**/tests/**/*.js --compilers js:babel-core/register",
    "lint": "xo",
    "build": "webpack --colors --progress",
    "dev": "npm run build -- --watch",
    "check": "python manage.py check",
    "check:deploy": "npm run check -- --deploy",
    "migrate": "python manage.py migrate",
    "serve": "python manage.py runserver",
    "start": "npm run build && npm run serve",
    "startdev": "npm run dev & npm run serve"
  },
  "xo": {
    "env": [
      "browser",
      "node"
    ],
    "extends": "xo-react"
  },
  "keywords": [
    "youtube",
    "videos"
  ],
  "author": "Alpha Shuro",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.6.5",
    "chai": "^3.5.0",
    "enzyme": "^2.1.0",
    "eslint-plugin-react": "^4.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "sinon": "^1.17.3",
    "webpack": "^1.12.14",
    "webpack-bundle-tracker": "0.0.93",
    "xo": "^0.13.0"
  }
}
